{
  "name": "base.react-native",
  "version": "0.0.1",
  "description": "React Native Template",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "jetify",
    "lint": "eslint .",
    "ios-offline": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "android-bundle": "bundletool build-apks --bundle=android/app/build/outputs/bundle/release/app.aab --output=android/app/build/outputs/bundle/release/app.apks",
    "android-offline": "cd android && ./gradlew bundleRelease && cd ..",
    "android-release": "react-native run-android --variant=release",
    "pods": "cd ios/ && rm -rf Pods/ && pod install && cd ..",
    "bootstrap": "rm -rf node_modules/ && yarn && yarn pods"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/slider": "^3.0.3",
    "@react-navigation/drawer": "5.8.6",
    "@react-navigation/native": "5.2.1",
    "@react-navigation/stack": "5.2.16",
    "axios": "0.19.2",
    "i18next": "19.6.3",
    "immutable": "4.0.0-rc.12",
    "moment": "2.27.0",
    "react": "16.13.1",
    "react-i18next": "11.3.1",
    "react-native": "0.63.1",
    "react-native-actionsheet": "^2.4.2",
    "react-native-animatable": "^1.3.3",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-image-picker": "2.3.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal-datetime-picker": "7.5.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-snackbar": "^2.2.3",
    "react-native-spinkit": "^1.5.0",
    "react-native-splash-screen": "^3.2.0",
    "react-navigation": "4.4.0",
    "react-redux": "7.2.0",
    "redux": "4.4.5",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.31",
    "@types/react": "16.9.26",
    "@types/react-native": "0.61.23",
    "@types/react-redux": "7.1.7",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "@typescript-eslint/typescript-estree": "2.25.0",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "jest": "24.9.0",
    "jetifier": "1.6.5",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-eject": "^0.1.2",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "globals": {
      "window": true,
      "__DEV__": false
    },
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|react-navigation)"
    ]
  }
}
